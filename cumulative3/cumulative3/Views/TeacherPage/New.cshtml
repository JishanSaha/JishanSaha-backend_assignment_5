<div id="containershow">
    <nav>
        <a class="navlink" href="/TeacherPage/List">Back To Teachers</a>
    </nav>
<h1>New Teacher</h1>
<form method="POST" action="/TeacherPage/Create">
    <div class="inputrow">
        <label for="Fname">First Name:</label>
        <input type="text" id="Fname" name="Fname" />

    </div>
    <div class="inputrow">
        <label for="Lname">Last Name:</label>
        <input type="text" id="Lname" name="Lname" />

    </div>
    <div class="inputrow">
        <label for="ENo">Employee Number:</label>
        <input type="text" id="ENo" name="ENo" />
    </div>
    <div class="inputrow">
        <label for="Salary">Salary:</label>
        <div>
            <input type="text" id="Salary" name="Salary" />
        </div>
    </div>
    <div class="inputrow">
        <input type="submit" value="Create Teacher" />
    </div>
</form>
</div>
